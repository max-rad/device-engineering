mixin clients(data)
  section.clients
    .container
      .clients__wrapper
        h2.title.title--h2!=data.title
        ul.list-reset.clients__list
          each item in data.list
            li.clients__item
              +client-card(item)
        +btn({href: "#", text: "Смотреть все"}).btn--border.clients__button

